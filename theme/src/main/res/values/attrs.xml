<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Theme_TextView">
        <attr name="android:textColor" />
        <attr name="android:textColorHint" />
        <attr name="android:textColorLink" />
        <attr name="android:textColorHighlight" />
    </declare-styleable>

    <declare-styleable name="Theme_Button">
        <attr name="android:background" />
        <attr name="android:textColor" />
        <attr name="backgroundTint" />
        <attr name="iconTint" />
        <attr name="strokeColor" />
        <attr name="rippleColor" />
    </declare-styleable>

    <declare-styleable name="Theme_CompoundButton">
        <attr name="android:textColor" />
        <attr name="buttonTint" />
    </declare-styleable>

    <declare-styleable name="Theme_Switch">
        <attr name="android:textColor" />
        <attr name="thumbTint" />
        <attr name="trackTint" />
    </declare-styleable>

    <declare-styleable name="Theme_CheckedTextView">
        <attr name="drawableStartCompat" />
        <attr name="android:checkMarkTint" />
    </declare-styleable>

    <declare-styleable name="Theme_ImageView">
        <attr name="android:background" />
        <attr name="tint" />
    </declare-styleable>

    <declare-styleable name="Theme_ShapeableImageView">
        <attr name="android:background" />
        <attr name="tint" />
        <attr name="strokeColor" />
    </declare-styleable>

    <declare-styleable name="Theme_EditText">
        <attr name="android:textColor" />
        <attr name="android:textColorHint" />
        <attr name="android:textColorLink" />
        <attr name="android:textColorHighlight" />
        <attr name="backgroundTint" />
    </declare-styleable>

    <declare-styleable name="Theme_Spinner">
        <attr name="backgroundTint" />
    </declare-styleable>

    <declare-styleable name="Theme_AppBarLayout">
        <attr name="android:background" />
    </declare-styleable>

    <declare-styleable name="Theme_CollapsingToolbarLayout">
        <!-- We need to set this value to make the collapsing content fade.-->
        <attr name="contentScrim" />
        <attr name="statusBarScrim" />
        <attr name="expandedTitleTextAppearance" />
        <attr name="collapsedTitleTextAppearance" />
    </declare-styleable>

    <declare-styleable name="Theme_Toolbar">
        <attr name="android:background" />
        <attr name="titleTextColor" />
        <attr name="subtitleTextColor" />
        <attr name="navigationIconTint" />
    </declare-styleable>

    <declare-styleable name="Theme_BottomAppBar">
        <attr name="backgroundTint" />
    </declare-styleable>

    <declare-styleable name="Theme_BottomNavigationView">
        <attr name="android:background" />
        <attr name="backgroundTint" />
        <attr name="itemIconTint" />
        <attr name="itemTextColor" />
        <attr name="itemRippleColor" />
    </declare-styleable>

    <declare-styleable name="Theme_Chip">
        <attr name="android:textColor" />
        <attr name="chipBackgroundColor" />
        <attr name="chipSurfaceColor" />
        <attr name="chipStrokeColor" />
        <attr name="rippleColor" />
        <attr name="closeIconTint" />
        <attr name="chipIconTint" />
        <attr name="checkedIconTint" />
    </declare-styleable>

    <declare-styleable name="Theme_FloatingActionButton">
        <attr name="tint" />
        <attr name="backgroundTint" />
        <attr name="rippleColor" />
    </declare-styleable>

    <declare-styleable name="Theme_ExtendedFloatingActionButton">
        <attr name="android:textColor" />
        <attr name="backgroundTint" />
        <attr name="iconTint" />
        <attr name="strokeColor" />
        <attr name="rippleColor" />
    </declare-styleable>

    <declare-styleable name="Theme_MaterialCardView">
        <attr name="cardBackgroundColor" />
        <attr name="cardForegroundColor" />
        <attr name="checkedIconTint" />
        <attr name="rippleColor" />
        <attr name="strokeColor" />
    </declare-styleable>

    <declare-styleable name="Theme_NavigationView">
        <attr name="android:background" />
        <attr name="itemIconTint" />
        <attr name="itemShapeFillColor" />
        <attr name="itemTextColor" />
    </declare-styleable>

    <declare-styleable name="Theme_TabLayout">
        <attr name="android:background" />
        <attr name="tabIconTint" />
        <attr name="tabTextColor" />
        <attr name="tabIndicatorColor" />
        <attr name="tabRippleColor" />
    </declare-styleable>

    <declare-styleable name="Theme_TextInputLayout">
        <attr name="android:textColorHint" />
        <attr name="hintTextColor" />
        <attr name="helperTextTextColor" />
        <attr name="errorTextColor" />
        <attr name="counterTextColor" />
        <attr name="counterOverflowTextColor" />
        <attr name="prefixTextColor" />
        <attr name="suffixTextColor" />
        <attr name="startIconTint" />
        <attr name="endIconTint" />
        <attr name="errorIconTint" />
        <attr name="boxStrokeColor" />
        <attr name="boxStrokeErrorColor" />
        <attr name="boxBackgroundColor" />
        <attr name="placeholderTextColor" />
    </declare-styleable>

    <declare-styleable name="Theme_TextInputEditText">
        <attr name="android:textColor" />
    </declare-styleable>

    <declare-styleable name="Theme_ProgressBar">
        <attr name="android:indeterminateTint" />
        <attr name="android:progressTint" />
    </declare-styleable>

    <declare-styleable name="Theme_SeekBar">
        <attr name="android:thumbTint" />
        <attr name="android:progressTint" />
    </declare-styleable>

    <declare-styleable name="Theme_Slider">
        <attr name="haloColor" />
        <attr name="thumbColor" />
        <attr name="thumbStrokeColor" />
        <attr name="tickColor" />
        <attr name="tickColorActive" />
        <attr name="tickColorInactive" />
        <attr name="trackColor" />
        <attr name="trackColorActive" />
        <attr name="trackColorInactive" />
    </declare-styleable>

    <declare-styleable name="Theme_ProgressIndicator">
        <attr name="indicatorColor" />
        <attr name="trackColor" />
    </declare-styleable>

    <declare-styleable name="Theme_NavigationRailView">
        <attr name="android:background" />
        <attr name="itemBackground" />
        <attr name="itemRippleColor" />
        <attr name="itemIconTint" />
        <attr name="itemTextColor" />
    </declare-styleable>

</resources>
